{
	"name": "node-starter-kit",
	"version": "1.0.0",
	"description": "node project",
	"main": "src/index.ts",
	"scripts": {
		"test": "jest",
		"dev": "nodemon src/main.ts",
		"type-check": "tsc --noEmit",
		"type-check:watch": "yarn type-check -- --watch",
		"build": "rm -rf dist && yarn build:js && yarn build:types",
		"build:types": "tsc -p . && tscpaths -p tsconfig.json -s ./src -o ./dist",
		"build:js": "yarn version --patch && babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --copy-files && cp ./package.json ./dist",
		"fire": "yarn build && cd dist && npm publish",
		"auth": "cd dist && npm login --always-auth && cd .."
	},
	"nodemonConfig": {
		"ignore": [
			"generated/*"
		],
		"execMap": {
			"ts": "babel-node src/main.ts --extensions \".js,.jsx,.ts,.tsx\""
		}
	},
	"author": {
		"name": "turgaysaba",
		"email": "turgaysaba@outlook.com",
		"url": "http://www.turgaysaba.com/"
	},
	"license": "MIT",
	"private": true,
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"node-fetch": "^2.6.0"
	},
	"devDependencies": {
		"@babel/cli": "7.10.5",
		"@babel/core": "7.11.4",
		"@babel/node": "7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-decorators": "^7.10.5",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-numeric-separator": "^7.10.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.11.0",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.8",
		"@types/body-parser": "^1.19.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"babel-extract-comments": "^1.0.0",
		"babel-plugin-module-resolver": "4.0.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-airbnb-typescript": "^7.2.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-functional": "^3.0.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"extract-comments": "^1.1.0",
		"glob": "^7.1.6",
		"jest": "^25.1.0",
		"nodemon": "2.0.2",
		"prettier": "2.0.5",
		"tscpaths": "^0.0.9",
		"typescript": "^3.9.5",
		"wait-on": "^3.3.0"
	}
}
